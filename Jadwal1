    public void selectData() {
        String kolom[] = {"NAMA BAND", "NO. TELP", "JAM MULAI", "JAM SELESAI", "TANGGAL"};
        DefaultTableModel dtm = new DefaultTableModel(null, kolom);
        String SQL = "SELECT * FROM t_pemesanan";
        ResultSet rs = Koneksi.executeQuery(SQL);
        try {
            while (rs.next()) {
                String nama = rs.getString(2);
                String no = rs.getString(3);
                String mulai = rs.getString(4);
                String selesai = rs.getString(5);
                String tanggal = rs.getString(6);
                String data[] = {nama, no, mulai, selesai, tanggal};
                dtm.addRow(data);
            }
        } catch (SQLException ex) {

            Logger.getLogger(Jadwal_02.class.getName()).log(Level.SEVERE, null, ex);
        }
        tbljadwal.setModel(dtm);
    }
}
